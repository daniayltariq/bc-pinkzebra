{"components/category/shop-by-price":"{{#and theme_settings.shop_by_price_visibility shop_by_price}}\n<div id=\"facetedSearch-navList\"\nclass=\"facetedSearch-navList\"\n>\n<div class=\"accordion accordion--navList\">\n    <div class=\"accordion-block shop-by-price\">\n        {{> components/faceted-search/faceted-search-navigation facet='price' title='Price'}}\n        <div id=\"facetedSearch-content--price\" class=\"accordion-content{{#unless start_collapsed}} is-open{{/unless}}\">\n            <ul class=\"navList\">\n                {{#each shop_by_price}}\n                <li class=\"navList-item\">\n                    <a \n                        {{#if selected }} \n                            class=\"navList-action navList-action--checkbox is-selected is-active\"\n                            role=\"status\"\n                            aria-live=\"assertive\"\n                    {{else}}\n                            class=\"navList-action navList-action--checkbox\"\n                    {{/if}}\n                    href=\"{{url}}\"\n                    >\n                    {{lang 'category.filter_price_range'}} {{low.formatted}} - {{high.formatted}}\n                    </a>\n                    <span class=\"price-filter-message aria-description--hidden\">{{lang 'category.filter_select_announcement'}}</span>\n                </li>\n                {{/each}}\n            </ul>\n        </div>\n    </div>\n</div>\n</div>\n{{/and}}\n","components/faceted-search/faceted-search-navigation":"<div class=\"accordion-nav-clear-holder\">\n    <button type=\"button\"\n         class=\"accordion-navigation toggleLink{{#unless start_collapsed}} is-open{{/unless}}\"\n         data-collapsible=\"#facetedSearch-content--{{dashcase facet}}\"\n         aria-label=\"{{lang 'search.faceted.toggleSection' title=title}}\"\n    >\n        <span>\n            {{title}}\n        </span>\n\n        <span class=\"indicator--desktop\">\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\n                <use href=\"#arrow-dropdown-down-download\" />\n            </svg>\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\n                <use href=\"#arrow-dropdown-down-download\" />\n            </svg>\n        </span>\n\n        <span class=\"indicator--mobile\">\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\n                <use href=\"#icon-expand-plus\" />\n            </svg>\n            <span class=\"accordion-indicator toggleLink-text toggleLink-text--on\">\n                â€”\n            </span>\n        </span>\n    </button>\n</div>\n"}
