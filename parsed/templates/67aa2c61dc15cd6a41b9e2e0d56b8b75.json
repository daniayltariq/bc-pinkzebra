{"components/common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\n    <ul class=\"navUser-section currencySelector\">\n        <li class=\"navUser-item\">\n            <a  class=\"navUser-action navUser-action--currencySelector has-dropdown\"\n                href=\"#\"\n                data-dropdown=\"currencySelection\"\n                aria-controls=\"currencySelection\"\n                aria-expanded=\"false\"\n            >\n                <i class=\"icon\" aria-hidden=\"true\">\n                    <svg>\n                        <use href=\"#icon-currency-{{{currency_selector.active_currency_code}}}\" />\n                    </svg>\n                </i>\n                <span class=\"currency-code\">\n                    {{#if currency_selector.active_currency_code '===' 'USD'}}\n                        {{lang 'common.countries_abbr.united_states'}}\n                    {{else}}\n                        {{lang 'common.countries_abbr.canada'}}\n                    {{/if}}\n                </span>\n                <i class=\"icon icon--dropdown\" aria-hidden=\"true\">\n                    <svg>\n                        <use href=\"#icon-arrow-dropdown-down\" />\n                    </svg>\n                </i>\n            </a>\n            <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n                {{#each currency_selector.currencies}}\n                    {{#if ../currency_selector.active_currency_code '!=' code }}\n                        <li class=\"dropdown-menu-item\">    \n                            <a  href=\"{{{switch_url}}}\"\n                                class=\"switch-currency\"\n                                currencyId=\"{{{id}}}\"\n                                data-currency-code=\"{{{code}}}\" {{#if cart_currency_switch_url.length '>' 0 }}\n                                data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\" {{/if}}\n                                data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                            >\n                                <i class=\"icon\" aria-hidden=\"true\">\n                                    <svg>\n                                        <use href=\"#icon-currency-{{{code}}}\" />\n                                    </svg>\n                                </i>\n                                {{#if code '===' 'USD'}}\n                                    <span class=\"currency-code\">{{lang 'common.countries_abbr.united_states'}}</span>\n                                {{else}}\n                                <span class=\"currency-code\">{{lang 'common.countries_abbr.canada'}}</span>\n                                {{/if}}\n                            </a>\n                        </li>\n                    {{/if}}\n                {{/each}}\n            </ul>\n        </li>\n    </ul>\n{{/if}}"}
