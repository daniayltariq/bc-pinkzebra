{"components/common/forms/multiline":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\">\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\n        {{#if required}}\n        <small>\n            {{#if show_required_text}}\n              {{lang 'common.required_text'}}\n            {{else}}\n              {{lang 'common.required'}}\n            {{/if}}\n        </small>\n        {{/if}}\n    </label>\n    <textarea name=\"{{name}}\"\n        id=\"{{id}}_input\"\n        data-label=\"{{label}}\"\n        rows=\"{{rows}}\"\n        aria-required=\"{{required}}\"\n        aria-labelledby=\"{{id}}\"\n        aria-live=\"polite\"\n        data-input\n        class=\"form-input\"\n        {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n    >{{value}}</textarea>\n</div>\n"}
