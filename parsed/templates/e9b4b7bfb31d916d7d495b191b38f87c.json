{"components/faceted-search/selected-facets":"<div class=\"facetedSearch-refineFilters sidebarBlock\">\n    <p>\n        {{lang 'search.faceted.selected.title'}}\n    </p>\n\n    {{#if items}}\n        <ul class=\"inlineList inlineList--labels\">\n            {{#each items}}\n                <li>\n                    <a href=\"{{url}}\"\n                       class=\"facetLabel\"\n                       rel=\"nofollow\"\n                       data-faceted-search-facet\n                    >\n                        {{#if facet '===' 'rating'}}\n                            {{> components/products/ratings rating=title}}\n                        {{else}}\n                            {{{sanitize title}}}\n                        {{/if}}\n\n                        <svg class=\"icon\">\n                            <use href=\"#icon-close\" />\n                        </svg>\n                    </a>\n                </li>\n            {{/each}}\n        </ul>\n    {{else}}\n        <p>{{lang 'search.faceted.selected.no-filters'}}</p>\n    {{/if}}\n</div>","components/products/ratings":"<span role=\"img\"\n    {{#if rating_target}}\n      aria-label=\"{{lang 'products.reviews.rating_aria_label' rating_target=rating_target current_rating=rating max_rating=5}}\"\n    {{else}}\n      aria-label=\"{{lang 'products.reviews.rating_aria_label' rating_target='Product' current_rating=rating max_rating=5}}\"\n    {{/if}}\n>\n    {{#for 1 5}}\n        {{#if ../this.rating '>=' $index}}\n            <span class=\"icon icon--ratingFull\">\n                <svg>\n                    <use href=\"#icon-star\" />\n                </svg>\n            </span>\n        {{else}}\n            <span class=\"icon icon--ratingEmpty\">\n                <svg>\n                    <use href=\"#icon-star\" />\n                </svg>\n            </span>\n        {{/if}}\n    {{/for}}\n</span>\n"}
